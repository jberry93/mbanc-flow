{
    "compilerOptions": {
        "target": "ESNext",
        "module": "ESNext",
        "lib": [
            "DOM",
            "ES2015"
        ],
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "isolatedModules": true,
        "noEmit": true,
        "types": ["astro/client"],
        "importsNotUsedAsValues": "error",
        "allowSyntheticDefaultImports": true
    }
}
